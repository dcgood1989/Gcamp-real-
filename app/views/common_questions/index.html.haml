.container
  .page-header
    %h1 Frequently Asked Questions
  %ul
    - @faq_array.each do |faq_hash|
      %li
        %u= link_to faq_hash[:question], "##{faq_hash[:slug]}"
  - @faq_array.each do |faq_hash|
    %h3{:id => faq_hash[:slug]}= faq_hash[:question]
    = faq_hash[:answer]
    %p    
      %a{:href => "#"} top
